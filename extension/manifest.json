{
  "name": "XFactChecker",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "XFactChecker.png"
  },
  "content_scripts": [
    {
      "matches": ["*://x.com/*"],
      "js": ["retrieveData.js"],
      "run_at": "document_end"
    }
  ]
}
